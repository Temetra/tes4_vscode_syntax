{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "TES4 Obscript",
	"patterns": [
		{
			"include": "#comments"
		},
		{
			"include": "#blocks"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#conditionals"
		},
		{
			"include": "#vartypes"
		},
		{
			"include": "#reserved-cs"
		},
		{
			"include": "#reserved-obse"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		}
	],
	"repository": {
		"comments": {
			"patterns": [
				{
					"begin": "(^\\s+)?(?=;)",
					"beginCaptures": {
						"1": {
							"name": "punctuation.whitespace.comment.leading.obscript"
						}
					},
					"end": "(?=$)",
					"patterns": [
						{
							"name": "comment.line.semicolon.obscript",
							"begin": "(;)",
							"beginCaptures": {
								"0": {
									"name": "punctuation.definition.comment.obscript"
								}
							},
							"end": "(?=$)"
						}
					]
				}
			]
		},
		"blocks": {
			"patterns": [
				{
					"begin": "(?i)(^(\\s+)?)(?=(begin|end)\\b)",
					"beginCaptures": {
						"1": {
							"name": "punctuation.whitespace.block.leading.obscript"
						}
					},
					"end": "(?=$)",
					"patterns": [
						{
							"name": "storage.type.function.obscript",
							"match": "(?i)(begin|end)"
						},
						{
							"name": "storage.type.function.obscript",
							"match": "(?i)_?(GameMode|MenuMode|OnActivate|OnActorEquip|OnActorUnequip|OnAdd|OnAlarm|OnAlarmVictim|OnDeath|OnDrop|OnEquip|OnHit|OnHitWith|OnKnockout|OnLoad|OnMagicEffectHit|OnMurder|OnPackageChange|OnPackageDone|OnPackageStart|OnReset|OnSell|OnStartCombat|OnTrigger|OnTriggerActor|OnTriggerMob|OnUnequip|ScriptEffectFinish|ScriptEffectStart|ScriptEffectUpdate|Function)"
						}
					]
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.obscript",
					"match": "(?i)\\b(scn|scriptname|set|let|call|return)\\b"
				},
				{
					"name": "support.variable.obscript",
					"match": "(?i)\\b(player|playerRef)\\b"
				}
			]
		},
		"conditionals": {
			"patterns": [
				{
					"name": "keyword.control.conditional.obscript",
					"match": "(?i)\\b(if|else|elseif|endif|while|foreach|loop|break|continue|label|goto)\\b"
				}
			]
		},
		"vartypes": {
			"patterns": [
				{
					"name": "support.type.primitive.obscript",
					"match": "(?i)\\b(short|long|float|ref|string_var|array_var)\\b"
				}
			]
		},
		"reserved-cs": {
			"patterns": [
				{
					"name": "support.function.obscript",
